{
  "name": "zoparga/laravel-szamlazzhu",
  "license": "MIT",
  "description": "Szamlazz.hu integration for Laravel",
  "keywords": [
    "laravel",
    "invoice",
    "receipt",
    "szamlazz.hu",
    "szamlazzhu",
    "currency"
  ],
  "authors": [
    {
      "name": "Forgács Bence",
      "email": "sykorax93@gmail.com"
    },
    {
        "name": "Papp Zoltán",
        "email": "zoltan@pappz.hu"
      }
  ],
  "require": {
    "php": "^7.1.3|^8.0 |^8.1 |^8.2",
    "ext-simplexml": "*",
    "ext-xmlwriter": "*",
    "guzzlehttp/guzzle": "6.* | 7.*",
    "laravel/framework": " 6.* | 7.* | 8.* | 9.* | 10.* | 11.* | 12.*"
  },
  "require-dev": {
    "orchestra/testbench": "*",
    "phpunit/phpunit": "*",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "files": [
    ],
    "psr-4": {
      "zoparga\\SzamlazzHu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "zoparga\\SzamlazzHu\\Tests\\": "test/"
    }
  },
  "suggest": {
    "illuminate/validation": "5.7.*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "aliases": {
      },
      "providers": [
        "zoparga\\SzamlazzHu\\Providers\\SzamlazzHuServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit"
  }
}
